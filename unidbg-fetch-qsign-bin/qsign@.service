[Unit]
Description=unidbg-fetch-qsign --basePath=txlib/%i
After=network.target

[Service]
Environment="JAVA_HOME=/usr/lib/jvm/default"
WorkingDirectory=/opt/qsign
ExecStart=/usr/bin/env bash bin/unidbg-fetch-qsign --basePath=txlib/%i
Restart=always
RestartSec=1s

[Install]
WantedBy=multi-user.target

